{
    "name": "mam4dali/laravel-wp-api",
    "description": "Laravel package for the Wordpress JSON REST API",
    "keywords": ["laravel", "package", "wordpress", "API", "REST"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Watkin",
            "email": "joseph.watkin@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "mam4dali\\LaravelWpApi\\ServiceProvider"
            ],
            "aliases": {
                "WpApi": "mam4dali\\LaravelWpApi\\Facade"
            }
        }
    },
    "require": {
        "php": ">=8.0.0",
        "illuminate/support": "8.*|9.*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {
            "mam4dali\\LaravelWpApi\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
